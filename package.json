{
  "name": "React-Redux-Skeleton",
  "description": "React Redux Skeleton",
  "author": "Vladimir Novick",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "concurrent --kill-others \"better-npm-run watch-client\" \"better-npm-run start-dev\"",
    "watch-client": "better-npm-run watch-client",
    "start-dev": "better-npm-run start-dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "betterScripts": {
    "start-dev": {
      "command": "nodemon . --kill-others",
      "env": {
        "NODE_ENV": "development",
        "PORT": 4545
      }
    },
    "watch-client": {
      "command": "nodemon --watch ./frontend/build_config --delay 250ms --exec webpack-dev-server --progress --colors --config ./frontend/build_config/webpack.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "dependencies": {
    "axios": "^0.7.0",
    "babel": "^6.5.2",
    "babel-core": "^6.7.6",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "better-npm-run": "0.0.4",
    "body-parser": "^1.15.0",
    "colors": "^1.1.2",
    "concurrently": "^1.0.0",
    "core-js": "^1.2.6",
    "css-loader": "^0.22.0",
    "css-mqpacker": "^4.0.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^0.9.1",
    "history": "^1.17.0",
    "lodash": "^3.10.1",
    "node-sass": "^3.4.2",
    "nodemailer": "^1.10.0",
    "nodemon": "^1.9.1",
    "postcss": "^5.0.19",
    "postcss-assets": "^4.0.1",
    "postcss-loader": "^0.8.2",
    "postcss-map-url": "^1.0.2",
    "postcss-merge-idents": "^2.1.5",
    "postcss-merge-longhand": "^2.0.1",
    "postcss-merge-rules": "^2.0.7",
    "postcss-svgo": "^2.1.2",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.2",
    "redux": "^3.0.4",
    "redux-effects-cookie": "^0.2.1",
    "redux-router": "^0.1.0",
    "redux-thunk": "^1.0.0",
    "request": "^2.47.0",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.12.1"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "^1.1.1",
    "better-npm-run": "0.0.4",
    "eslint": "^1.9.0",
    "eslint-plugin-react": "^3.8.0",
    "mocha": "^2.3.3",
    "redux-logger": "^2.0.4",
    "webpack-dev-server": "^1.14.1"
  }
}
