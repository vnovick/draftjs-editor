{
  "name": "React-Redux-Skeleton",
  "description": "React Redux Skeleton",
  "author": "Vladimir Novick",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "start-dev": "better-npm-run start-dev",
    "start-prod": "better-npm-run start-prod",
    "watch-client": "better-npm-run watch-client",
    "build": "webpack --config ./frontend/webpack.config.js"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node . --kill-others",
      "env": {
        "NODE_PATH": "./server",
        "NODE_ENV": "production",
        "PORT": 3500
      }
    },
    "start-dev": {
      "command": "node . --kill-others",
      "env": {
        "NODE_PATH": "./server",
        "NODE_ENV": "development",
        "PORT": 3500
      }
    },
    "watch-client": {
      "command": "webpack-dev-server --progress --colors --config ./frontend/webpack.config.js",
      "env": {
        "NODE_PATH": "frontend"
      }
    }
  },
  "dependencies": {
    "axios": "^0.7.0",
    "babel": "^6.1.18",
    "babel-core": "^6.3.13",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^6.1.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-2": "^6.3.13",
    "babel-register": "^6.2.0",
    "babel-runtime": "^6.2.0",
    "concurrently": "^1.0.0",
    "core-js": "^1.2.6",
    "css-loader": "^0.22.0",
    "ejs": "^2.3.4",
    "express": "~3.3.4",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "forever": "^0.10.11",
    "history": "^1.17.0",
    "lodash": "^3.10.1",
    "node-sass": "^3.4.1",
    "nodemailer": "^1.10.0",
    "react": "^0.14.2",
    "react-addons-pure-render-mixin": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.2",
    "redux": "^3.0.4",
    "redux-effects-cookie": "^0.2.1",
    "redux-immutable": "^1.3.7",
    "redux-router": "^0.1.0",
    "redux-thunk": "^1.0.0",
    "request": "^2.47.0",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.12.1"
  },
  "devDependencies": {
    "better-npm-run": "0.0.4",
    "babel-plugin-react-transform": "^1.1.1",
    "eslint": "^1.9.0",
    "eslint-plugin-react": "^3.8.0",
    "mocha": "^2.3.3",
    "redux-logger": "^2.0.4"
  },
  "engine": "node >= 0.4.1"
}
